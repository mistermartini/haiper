{
    "name": "haiper",
    "version": "0.1.1",
    "description": "Haiku on Paper",
    "author": "Priyatam Mudivarti",
    "repository": {
        "type": "git",
        "url": "git://github.com/priyatam/haiper.git"
    },
    "bugs": {
        "url": "https://github.com/priyatam/haiper/issues"
    },
    "keywords": [
        "haiku",
