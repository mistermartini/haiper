        "gulp-minify-css": "0.3.1",
        "gulp-jshint": "1.5.3",
        "gulp-imagemin": "~0.1.5",
        "gulp-util": "^2.2.14",
        "gulp-uglify": "0.2.1",
        "gulp-open": "~0.2.8",
        "gulp-notify": "~1.2.4",
        "gulp-clean": "0.2.4",
        "gulp-rename": "1.2.0",
        "gulp-concat": "2.2.0",
        "gulp-changed": "~0.3.0",
        "browserify": "~3.36.0",
        "browserify-shim": "~3.4.1",
        "connect": "~2.14.3",
